				<choice name="enemy">
					<soundset name="JapaneseSoheiAttack"/>
				</choice>
				<choice name="build"/>
				<choice name="Huntable"/>
				<choice name="Tree"/>
				<choice name="AbstractMine"/>
				<choice name="AbstractFarm"/>
