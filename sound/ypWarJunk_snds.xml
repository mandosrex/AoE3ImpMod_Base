<?xml version="1.0" encoding="utf-8"?>

<protounitsounddef>
	<protounit name="ypWarJunk">
		<soundtype name="Death">
			<soundset name="ShipDeath"/>
		</soundtype>
		<soundtype name="Creation">
			<soundset name="ShipBirth"/>
		</soundtype>
		<soundtype name="Select">
			<soundset name="ChineseChuKoNuSelect"/>
		</soundtype>
		<soundtype name="Acknowledge">
			<targetlogic>
				<choice name="default">
					<soundset name="ChineseChuKoNuAcknowledge"/>
				</choice>
				<choice name="enemy">
					<soundset name="ChineseChuKoNuAttack"/>
				</choice>
				<choice name="build"/>
				<choice name="Huntable"/>
				<choice name="Tree"/>
				<choice name="AbstractMine"/>
				<choice name="AbstractFarm"/>
				<choice name="AbstractResourceCrate"/>
				<choice name="AbstractFruit"/>
			</targetlogic>
		</soundtype>
		<soundtype name="Exists">
			<soundset name="AmbienceShip"/>
		</soundtype>
	</protounit>
</protounitsounddef>