<?xml version="1.0" encoding="utf-8"?>

<protounitsounddef>
	<protounit name="Cavalaria">
		<soundtype name="Select">
			<soundset name="PortugueseLightCavalrySelect"/>
		</soundtype>
		<soundtype name="Grunt">
			<soundset name="CavalryGrunt"/>
		</soundtype>
		<soundtype name="Death">
			<soundset name="GenericMaleDeath"/>
		</soundtype>
		<soundtype name="Creation">
			<soundset name="MilitaryBirth"/>
		</soundtype>
		<soundtype name="Acknowledge">
			<targetlogic>
				<choice name="default">
					<soundset name="PortugueseLightCavalryAcknowledge"/>
				</choice>
				<choice name="enemy">
					<soundset name="PortugueseLightCavalryAttack"/>
				</choice>
				<choice name="Tree"/>
				<choice name="build">
					<soundset name="PortugueseLightCavalryAcknowledge"/>
				</choice>
			</targetlogic>
		</soundtype>
	</protounit>
</protounitsounddef>