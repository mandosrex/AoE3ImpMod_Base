<?xml version="1.0" encoding="utf-8"?>

<protounitsounddef>
	<protounit name="ypMonkJapanese">
		<soundtype name="Select">
			<soundset name="JapaneseMonkSelect"/>
		</soundtype>
		<soundtype name="Grunt">
			<soundset name="GenericMaleGrunt"/>
		</soundtype>
		<soundtype name="Death">
			<soundset name="GenericMaleDeath"/>
		</soundtype>
		<soundtype name="Creation">
			<soundset name="JapaneseMonkRevived"/>
		</soundtype>
		<soundtype name="Acknowledge">
			<targetlogic>
				<choice name="default">
					<soundset name="JapaneseMonkAcknowledge"/>
				</choice>
				<choice name="enemy">
					<soundset name="JapaneseMonkAttack"/>
				</choice>
				<choice name="build">
					<soundset name="JapaneseMonkAcknowledge"/>
				</choice>
				<choice name="Huntable"/>
				<choice name="Tree"/>
				<choice name="AbstractMine"/>
				<choice name="AbstractFarm"/>
				<choice name="AbstractResourceCrate"/>
				<choice name="AbstractFruit"/>
			</targetlogic>
		</soundtype>
		<soundtype name="Claim">
			<soundset name="JapaneseMonkClaim"/>
		</soundtype>
		<soundtype name="KnockOut">
			<soundset name="JapaneseMonkDisabled"/>
		</soundtype>
		<soundtype name="KnockOutRevived">
			<soundset name="JapaneseMonkRevived"/>
		</soundtype>
	</protounit>
</protounitsounddef>